<p id="notice"><%= notice %></p>

<h1>Episodes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Link</th>
      <th>Description</th>
      <th>Show notes</th>
      <th>Published at</th>
      <th>Audio file url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @episodes.each do |episode| %>
      <tr>
        <td><%= episode.name %></td>
        <td><%= episode.link %></td>
        <td><%= episode.description %></td>
        <td><%= episode.show_notes %></td>
        <td><%= episode.published_at %></td>
        <td><%= episode.audio_file_url %></td>
      </tr>
    <% end %>
  </tbody>
</table>
